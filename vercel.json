{
    "rewrites": [
      {
        "source": "/spaceon/:path*",
        "destination": "http://janghong.asia/:path*"
      },
      {
        "source": "/api/naver-local",
        "destination": "/api/naver-local.js"
      },
      {
        "source": "/api/ncp-reverse",
        "destination": "/api/ncp-reverse.js"
      }
    ],
    "functions": {
